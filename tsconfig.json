{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "sourceMap": true,
    "moduleResolution": "nodenext",
    "module": "nodenext",
    "target": "es6",
    "skipLibCheck": true,
    "lib": [
      "dom",
      "es6",
      "scripthost",
      "es2016",
      "es2020.Promise"
    ],
    "jsx": "react",
    "noEmitHelpers": false,
    "noUnusedLocals": false,
    "allowUnreachableCode": true,
    "allowJs": true,
    "strictNullChecks": false
  }
}
